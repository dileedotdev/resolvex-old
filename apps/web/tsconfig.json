{
	"extends": "../../tsconfig.base.json",
	"compilerOptions": {
		"target": "es5",
		"incremental": true,
		"esModuleInterop": true,
		"lib": [
			"dom",
			"dom.iterable",
			"esnext"
		],
		"plugins": [
			{
				"name": "next"
			}
		],
		"paths": {
			"~/*": [
				"./*"
			]
		},
	},
	"include": [
		"next-env.d.ts",
		"**/*.ts",
		"**/*.tsx",
		".next/types/**/*.ts",
		"env.ts.mjs"
	],
	"exclude": [
		"node_modules"
	]
}