{
	"name": "resolvex",
	"private": true,
	"scripts": {
		"build": "turbo run build",
		"dev": "turbo run dev",
		"lint": "rome check . && turbo run lint",
		"lint:fix": "rome check --apply-unsafe . && turbo run lint:fix",
		"type:check": "tsc --noEmit && turbo run type:check",
		"format": "rome format --write . && turbo run format"
	},
	"devDependencies": {
		"rome": "^12.1.3",
		"turbo": "latest",
		"typescript": "^5.1.6"
	},
	"packageManager": "pnpm@8.0.0"
}
